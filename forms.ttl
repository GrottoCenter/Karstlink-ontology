#  "city": "Accous",
#  "latitude": 42.942035715037,
#  "longitude": -0.51369667053223,
schema:polygon
dc:isPartOf
dc:hasPart
dc:rights
karstlink:areaType
) .

karstlink:forms-BibliographicResource
a form:specification ;
  form:classDomain dc:BibliographicResource ;
  rdfs:label "Formulaire Bibliographie"@fr ;
  rdfs:label "Bibliographic Resource form"@en ;
  form:showProperties (
dc:title
dc:type
dc:subject
gn:countryCode
dc:language
dc:publisher
dc:creator
dc:date
dc:format
dc:identifier
dc:source
dc:relation
dc:isReferencedBy
dc:isPartOf
dc:hasPart
dc:rights
) .

karstlink:forms-Person
a form:specification ;
  form:classDomain foaf:Person ;
  rdfs:label "Formulaire "@fr ;
  rdfs:label " form"@en ;
  form:showProperties (
foaf:firstName
foaf:lastName
foaf:nick
foaf:member
gn:countryCode
dc:creator
dc:relation
karstlink:visited
) .

karstlink:forms-Organization
a form:specification ;
  form:classDomain foaf:Organization ;
  rdfs:label "Formulaire "@fr ;
  rdfs:label " form"@en ;
  form:showProperties (
rdfs:label
gn:alternateName
foaf:mbox
foaf:homepage
schema:streetAddress
schema:postalCode
schema:addressLocality
schema:addressCountry
geo:lat
geo:long
dwc:coordinatePrecision
dc:isPartOf
dc:hasPart
foaf:member
dc:creator
dc:publisher
dc:relation
karstlink:visited
) .

karstlink:forms-Observation
a form:specification ;
  form:classDomain karstlink:Observation ;
  rdfs:label "Formulaire "@fr ;
  rdfs:label " form"@en ;
  form:showProperties (
dc:creator
dc:relation
dc:rights
dc:isPartOf
dwc:Occurrence
rdfs:label
dc:creator
dc:relation
dc:rights
dc:isPartOf
dwc:recordedBy
dwc:identifiedBy
dwc:dateIdentified
dwc:eventDate
dwc:individualCount
dwc:associatedTaxa
) .

karstlink:forms-LicenseDocument
a form:specification ;
  form:classDomain dc:LicenseDocument ;
  rdfs:label "Formulaire "@fr ;
  rdfs:label " form"@en ;
  form:showProperties (
cc:attributionName
karstlink:licenceType
dc:created
dc:modified
cc:attributionURL
) .


karstlink:forms-Access
